@{
    TempData["Title"] = "Info";
}
<h1>@TempData["Message"]</h1>
<br />
<p>There are @TempData["StudentCount"] number of students.</p>
<br />
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Student Id</th>
            <th>Name</th>
            <th>City</th>
            <th>Gender</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in TempData["StudentList"] as IList<Student>) 
        {
            <tr>
                <td>@student.StudentId</td>
                <td>@student.Name</td>
                <td>@student.city</td>
                <td>@student.Gender</td>
            </tr>
        }
    </tbody>
</table>


